{
  "name": "to-do-list",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && npm run start:back",
    "build": "ng build",
    "start:app": "ng serve",
    "watch:app": "ng build --watch --configuration development",
    "test:app": "ng test",
    "start:back": "cross-env NODE_ENV=production node ./src/backend/bin/server.js",
    "start:back:dev": "cross-env NODE_ENV=development nodemon ./src/backend/bin/server.js",
    "lint": "eslint **/*.{js,json}",
    "lint:fix": "eslint --fix **/*.{js,json}",
    "dev:debug": "node --inspect ./src/backend/bin/server.js",
    "test:back": "cross-env NODE_ENV=test jest --no-cache --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@sendgrid/mail": "^7.4.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "gravatar": "^1.8.1",
    "helmet": "^4.4.1",
    "jimp": "^0.16.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mailgen": "^2.0.14",
    "mongoose": "^5.12.2",
    "mongoose-paginate-v2": "^1.3.16",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.22",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5",
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
